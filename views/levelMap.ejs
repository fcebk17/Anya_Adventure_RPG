        <div class="col-1">
            <label>約兒自我介紹</label>
            <img id="img" src="/img/levelmap/5yor.png">
        </div>
        <div class="col-1">
            <label>跟彭德出乃玩</label>
            <img id="img" src="/img/levelmap/7walkdog.png">
        </div>
        <div class="col-1"></div>
        <div class="col-1">
            <label>彭德自我介紹</label>
            <img id="img" src="/img/levelmap/9bond.png">
        </div>
        <div class="col-1">
            <label>星力考驗</label>
            <img id="img" src="/img/levelmap/11starexam.png">
        </div>
    </div>
    <div class="row"> <!--row3-->
        <div class="col-1"></div>
        <div class="col-1">
            <label>洛伊德給獎勵</label>
            <img id="img" src="/img/levelmap/4loyidaward.png">
        </div>
        <div class="col-1"></div>
        <div class="col-1"></div>
        <div class="col-1">
            <label>約兒給獎勵</label>
            <img id="img" src="/img/levelmap/8yoraward.png">
        </div>
        <div class="col-1"></div>
        <div class="col-1"></div>
        <div class="col-1">
            <label>星力考驗(plus+)</label>
            <img id="img" src="/img/levelmap/12starexamplus.png">
        </div>
    </div>
</div>


