<% layout('layout') %>
<link rel="stylesheet" href="/css/levelmap.css"/>
<div class="container">
    <br>
    <div class="row">
        <h1>關卡地圖</h1>
    </div>
    <div class="row"> <!--row1-->
        <div class="col-1"></div>
        <div class="col-1">
            <label>算數學 - 簡單</label>
